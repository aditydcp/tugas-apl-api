<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%= users[i]._id %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].posisi %></td>
    <td>
        <a href="/update-user?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-edit"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
            <span class="text-gradient"><i class="fas fa-user-times"></i></span>
        </a>
    </td>
</tr>
<% } %>